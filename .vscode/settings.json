{
	"cSpell.words": [
		"anonymization",
		"createsuperuser",
		"esbuild",
		"makemigrations",
		"pytest",
		"runserver",
		"shadcn",
		"Specialities",
		"Speciality",
		"UKMA",
		"venv",
		"Активний",
		"Анонімний",
		"Весна",
		"Вибірковий",
		"відгук",
		"відгуки",
		"Відгуків",
		"жовтня",
		"Завантаження",
		"Завершено",
		"Заплановано",
		"Корисність",
		"курсу",
		"Літо",
		"немає",
		"Обов'язковий",
		"орієнтований",
		"Осінь",
		"Поки",
		"Професійно",
		"Складність",
		"Спеціальності",
		"студент",
		"студентів",
		"цього"
	],
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.organizeImports.biome": "explicit",
		"source.organizeImports.ruff": "explicit",
		"source.fixAll": "explicit"
	},
	"files.watcherExclude": {
		"**/routeTree.gen.ts": true
	},
	"search.exclude": {
		"**/routeTree.gen.ts": true
	},
	"files.readonlyInclude": {
		"**/routeTree.gen.ts": true
	},
	"python.analysis.typeCheckingMode": "standard",
	"[python]": {
		"editor.defaultFormatter": "charliermarsh.ruff"
	},
	"[javascript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[typescript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[json]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[css]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"files.associations": {
		"*.css": "tailwindcss"
	},
	"biome.lsp.bin": "./src/webapp/node_modules/.bin/biome",
	"biome.configurationPath": "./src/webapp/biome.json",
	"python.testing.pytestArgs": ["src/backend"],
	"python.testing.unittestEnabled": false,
	"python.testing.pytestEnabled": true
}
